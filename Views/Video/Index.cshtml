@using WebApplication2.Models;
@model List<Video>
@{
    ViewBag.Title = "Course Videos";
}
<div class="jumbotron">

</div>
<h2>Course Videos</h2>

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="sparkline12-list mg-b-30">
        <div class="sparkline12-hd">
            <div class="main-sparkline12-hd">
                <h1>Hover Table</h1>
            </div>
        </div>
        <div class="sparkline12-graph">
            <div class="static-table-list">
                <table class="table hover-table">
                    <thead>
                        <tr>
                            <th>Video ID</th>
                            <th>Course ID</th>
                            <th>Subjects ID</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in ViewBag.liste9)
                        {
                            <tr>
                                <td>@item.videoID</td>
                                <td>@item.kursID</td>
                                <td>@item.konuID</td>
                                <td><a href="/Video/videoSil/@item.videoID" class="btn btn-danger"> Delete </a></td>

                                @*<td>
                                        <span class="label label-success">Complete</span>
                                    </td>*@
                            </tr>
                        }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
