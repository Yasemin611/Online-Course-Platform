@using WebApplication2.Models;
@model List<Kurs_Programi_Yoneticisi>
@{
    ViewBag.Title = "yoneticiSil";
}

<h2>yoneticiSil</h2>

<div class="row">
    <div class="col-md-12">
        <div class="block-heading">
            <div class="main-txt h2"> OPERATIONS OF DELETING ADMIN </div>
        </div>
        <div class="block-content-outer">
            <div class="block-content-inner">
                <form action="/Kurs_Programi_Yoneticisi/yoneticiSil" method="post">
                    <input type="hidden" name="yoneticiID" value="@Model.yoneticiID" />
                    <label> Delete admin named @Model.yoneticiAdi @Model.yoneticiSoyadi ?</label>
                    <input class="btn-danger rounded" type="submit" value="Yes" />
                </form>
            </div>
        </div>
    </div>
</div>
