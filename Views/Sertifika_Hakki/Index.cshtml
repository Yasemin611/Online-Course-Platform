@using WebApplication2.Models;
@model List<Sertifika_Hakki>
@{
    ViewBag.Title = "Trainees According to Certificates";
}
<div class="jumbotron">
</div>
<h2>Trainees According to Certificates</h2>

<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="sparkline12-list mg-b-30">
        <div class="sparkline12-hd">
            <div class="main-sparkline12-hd">
                <h1>Hover Table</h1>
            </div>
        </div>
        <div class="sparkline12-graph">
            <div class="static-table-list">
                <table class="table hover-table">
                    <thead>
                        <tr>
                            <th>Score</th>
                            <th>Trainee Name</th>
                            <th>Trainee Surname</th>
                            <th>Certificate Name</th>
                            <th>Certication Status</th>

                            @*<th>Delete</th>
        <th>Update</th>*@
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.puan</td>
                                <td>@item.kursiyerAdi</td>
                                <td>@item.kursiyerSoyadi</td>
                                <td>@item.sertifikaAdi</td>
                                <td>@item.sertfHakki</td>

                            </tr>
                                    }


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>